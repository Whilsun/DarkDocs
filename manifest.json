{
  "name": "Dark Docs",
  "author": "Whilsun, The Anarthrous",
  "version": "1.2",
  "description": "Dark Theme for Docs(?), Drive(?), Git(?)",
  "permissions": [
    "*://docs.google.com/*",
    "tabs",
    "*://docs.microsoft.com/*",
    "tabs"

  ],
  "browser_action": {
    "default_icon": {
      "20": "images/favicon.png",
      "40": "images/favicon.png"
    },
    "default_title": "Color Changer",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://docs.google.com/*"
      ],
      "js": [ "contentScript.js" ],
      "run_at": "document_idle",
      "all_frames":  true
    }
  ]
}